{
  "name": "pdf-publish-system",
  "version": "1.0.0",
  "description": "债权公告发布助手 - 管理端和用户端",
  "private": true,
  "workspaces": [
    "admin",
    "viewer",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:admin\" \"npm run dev:viewer\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:admin": "npm run dev --workspace=admin",
    "dev:viewer": "npm run dev --workspace=viewer",
    "build": "npm run build --workspaces",
    "build:server": "npm run build --workspace=server",
    "build:admin": "npm run build --workspace=admin",
    "build:viewer": "npm run build --workspace=viewer"
  },
  "keywords": [
    "pdf",
    "upload",
    "preview"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
